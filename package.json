{
  "name": "vite-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "rimraf dev-dist && cross-env DEBUG=vite-plugin-pwa:* SW_DEV=true vite --force",
    "build": "cross-env DEBUG=vite-plugin-pwa:* BASE_URL=/ VITE_SOURCE_MAP=true VITE_RELOAD_SW=true VITE_SW=true vite build",
    "preview": "vite preview",
    "lint": "eslint ./src/*.ts{,x} --fix",
    "format": "prettier --config .prettierrc --write",
    "test": "jest --config scripts/jest.config.js",
    "test:coverage": "jest --config scripts/jest.config.js --collectCoverage",
    "codegen": "graphql-codegen --config scripts/gqlcodegen/query.js && yarn format ./src/gqlcodegen/**/*.ts"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn lint ./",
      "yarn format ./",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.11",
    "dayjs": "^1.11.9",
    "dompurify": "^3.0.5",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "immer": "^10.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lazy-load-image-component": "^1.6.0",
    "react-router": "^6.3.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^6.3.0",
    "regenerator-runtime": "^0.13.9",
    "tinycolor2": "^1.6.0",
    "use-immer": "^0.9.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/near-operation-file-preset": "^1.17.13",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@graphql-codegen/typescript-react-apollo": "2.2.5",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^1.3.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "husky": "^4.2.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "ts-jest": "^26.5.4",
    "typescript": "^4.6.3",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.16.4",
    "vite-plugin-replace": "^0.1.1",
    "vite-tsconfig-paths": "^4.2.0",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-window": "^6.5.4"
  }
}
